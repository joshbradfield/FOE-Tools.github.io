div
  h3.title.is-3 {{$t(i18nPrefix + 'choose_gb.title')}}

  div.tile.is-ancestor(v-for="items of GBsByAge")
    div.tile.is-parent(v-for="item of items")
      article.tile.is-child.box(v-if="item !== null")
        p.title.is-4(:class="getGbStyle(item.key)") {{$t('foe_data.age.' + item.key)}}
        div.content
          ul
            li(v-for="gb in item.gbs")
              nuxt-link(:to="{ name: 'GbInvestment', params: { gb } }" exact) {{$t('foe_data.gb.' + gb)}}
